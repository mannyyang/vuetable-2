  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "globals": {
      "VERSION": "dummy4test"
    },
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  },
  "bugs": {
    "url": "https://github.com/mannyyang/vuetable-3/issues"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "homepage": "https://github.com/mannyyang/vuetable-3#readme",
  "keywords": [
    "vue",
    "component",
    "table",
    "datatable",
    "pagination-components",
    "json"
  ],
  "license": "MIT",
  "lint-staged": {
    "src/*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "readme": "https://github.com/mannyyang/vuetable-3#readme",
  "repository": {
